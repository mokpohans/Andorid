<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/batang"
    tools:context=".NORexplainActivity">

    <ImageView
        android:id="@+id/nor_explain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/exforand" />

    <ImageButton
        android:id="@+id/nor_back"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="303dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="633dp"
        android:adjustViewBounds="true"
        android:background="@drawable/batang"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/nor_explain"
        app:layout_constraintEnd_toEndOf="@+id/nor_explain"
        app:layout_constraintStart_toStartOf="@+id/nor_explain"
        app:layout_constraintTop_toTopOf="@+id/nor_explain"
        app:srcCompat="@drawable/backward" />
</androidx.constraintlayout.widget.ConstraintLayout>